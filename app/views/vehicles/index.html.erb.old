<h1>Listing vehicles</h1>

<table>
  <thead>
    <tr>
      <th>Reg no</th>
      <th>Chassis no</th>
      <th>Engine no</th>
      <th>Reg on</th>
      <th>Manufacturer year</th>
      <th>Manufacturer</th>
      <th>Model</th>
      <th>Category</th>
      <th>Aquired on</th>
      <th>Price</th>
      <th>Contract</th>
      <th>Status</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @vehicles.each do |vehicle| %>
      <tr>
        <td><%= vehicle.reg_no %></td>
        <td><%= vehicle.chassis_no %></td>
        <td><%= vehicle.engine_no %></td>
        <td><%= vehicle.reg_on %></td>
        <td><%= vehicle.manufacturer_year %></td>
        <td><%= vehicle.manufacturer_id %></td>
        <td><%= vehicle.model %></td>
        <td><%= vehicle.category_id %></td>
        <td><%= vehicle.aquired_on %></td>
        <td><%= vehicle.price %></td>
        <td><%= vehicle.contract_id %></td>
        <td><%= vehicle.status_id %></td>
        <td><%= link_to 'Show', vehicle %></td>
        <td><%= link_to 'Edit', edit_vehicle_path(vehicle) %></td>
        <td><%= link_to 'Destroy', vehicle, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vehicle', new_vehicle_path %>
